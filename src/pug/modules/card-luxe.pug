.card-luxe
	.swiper-container
		.swiper-wrapper
			.swiper-slide
				img(src=require('/img/luxe.jpg').default, alt="")
			.swiper-slide
				img(src=require('/img/luxe-2.png').default, alt="")
			.swiper-slide
				img(src=require('/img/luxe-3.png').default, alt="")
			.swiper-slide
				img(src=require('/img/luxe-4.png').default, alt="")
		.swiper-pagination
		.swiper-button-next
		.swiper-button-prev
	.card-luxe__title.luxe-title
		h3.luxe-title__left
			span №
			| 888
			span люкс
		p.luxe-title__right 9 990₽
			span в сутки
	.card-luxe__reviews
		.rate-elements__item.card-luxe__rating
			select.rate-elements__rating
				option(value='1') 1
				option(value='2') 2
				option(value='3') 3
				option(value='4') 4
				option(value='5') 5
		.card-luxe__text 145
			span Отзывов
