mixin checkboxElements
  - const checkbox = require('../../components/checkbox/checkbox.json');
  .form-elements__checkbox
    .form-elements__checkbox-item
      +checkbox('Checkbox Buttons', checkbox)
    .radio
      .radio__title Radio Buttons
      .radio__item
        +radio([
          {
            id: 1,
            checked: true,
            gender: 'Мужчина'
          },
          {
            id: 2,
            gender: 'Женщина'
          }
        ])
    .toggle
      .toggle__title Toggle
      .form-elements__toggle
        +toggle([
          {
            id: 1,
            checked: true,
            text: 'Получать спецпредложения'
          }
        ])
      .form-elements__toggle
        +toggle([
          {
            id: 2,
            text: 'Получать спецпредложения'
          }
        ])
    .like-elements
      .like-elements__title like button
      .like-elements__inner
        +like([
          {
            counter: 2
          },
          {
            counter: 12,
            buttonClasses: 'like-elements__btn-like2'
          }
        ])
    .rating
      .rating__title rate button
      .rating__inner
        +rating([
          {
            rating: '4'
          },
          {
            rating: '5'
          }
        ])