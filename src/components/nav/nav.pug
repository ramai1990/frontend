mixin nav(navigations)
  nav.nav
    ul.nav__list
      each nav in navigations
        - const { submenu, name, items } = nav;
        if submenu
          li.nav__item.submenu
            a.nav__link.nav__link_dropdown= name
            ul.submenu__list
              each item in items
                - const { name } = item;
                li.submenu__item
                  a(href="https://ramai1990.github.io/frontend/404.html").submenu__link= name
        else
          li.nav__item
            a(href="https://ramai1990.github.io/frontend/404.html").nav__link= name