mixin nav(navigations)
  nav.nav
    ul.nav__list
      each nav in navigations
        if nav.submenu
          li.nav__item.submenu
            a.nav__link.nav__link_dropdown= nav.name
            ul.submenu__list
              each item in nav.items
                li.submenu__item
                  a(href="https://ramai1990.github.io/frontend/404.html").submenu__link= item.name
        else
          li.nav__item
            a(href="https://ramai1990.github.io/frontend/404.html").nav__link= nav.name