mixin rangeElements
  - const pagination = require('../../components/pagination/pagination.json');
  .form-elements__range
    +rangeSlider([
      {
        title: 'Range Slider'
      }
    ])
    .button-elements
      h3.button-elements__title.elements-title Buttons
      +btn({
        theme: 'primary',
        text: 'CLICK ME',
        buttonClass: 'button-elements__btn'
      })
      +btn({
        theme: 'border',
        text: 'CLICK ME',
        buttonClass: 'header-btns__signin'
      })(class='button-elements__border')
      .button-elements__item
        +btn({
          text: 'CLICK ME',
          href: 'form-elements.html'
        })
      +btn({
        theme: 'arrow',
        text: 'перейти к оплате'
      })
    h3.pagination-elements__title.elements-title pagination
    +pagination({
      text: '1 – 12 из 100+ вариантов аренды',
      list: pagination
    })