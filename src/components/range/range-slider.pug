mixin rangeSlider(items)
  each item in items
    - const { title, classRange, span } = item;
    .range-elements
      h3.range-elements__title.elements-title= title
      .range-elements__wrapper
        input.input-with-keypress-0(type='text')
        span -
        input.input-with-keypress-1(type='text')
      .slider-keypress
      span= span